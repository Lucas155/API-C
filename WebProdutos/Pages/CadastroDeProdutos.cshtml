@page
@model WebProdutos.Model.Produtos
@{
    ViewData["Title"] = "CadastroDeProdutos";
}

@using (Html.BeginForm("Action", "Controller", new { }, FormMethod.Post))
{
    <div class="form-group">
        @Html.TextBoxFor(x => x.id, "000-000", new { @class = "form-control", id = "id" })
    </div>
    <div class="form-group">
        
        @Html.TextBoxFor(x => x.Produto, "", new { @class = "form-control", id = "produto", autocompleto = "off", autofocus = "true" })
    </div>
    <div class="form-group">
        @Html.TextBoxFor(x => x.preco, "", new { @class = "form-control", id = "preco", autocompleto = "off", autofocus = "true" })
    </div>
    <div class="form-group">
        @Html.TextBoxFor(x => x.sku, "", new { @class = "form-control", id = "sku", autocompleto = "off", autofocus = "true" })
    </div>

    <div class="form-group">
        <input type="submit" id="div_Button" value="Finish Register" />
    </div>
}

